#wiki
  .wiki-list
    display flex
    flex-wrap wrap
    > li
      flex-basis 50%
      width 160px
      height 160px
      padding 8px 8px
      > div
        width 100%
        height 100%
        background-position center
        background-size: cover;
        background-repeat no-repeat
    > li:nth-child(1)
      padding-left 16px
      > div
        background-image url('./img/1-智金库.png')
    > li:nth-child(2)
      padding-right 16px
      > div
        background-image url('./img/2-智金典.png')
    > li:nth-child(3)
      padding-left 16px
      > div
        background-image url('./img/3-海外案例.png')
    > li:nth-child(4)
      padding-right 16px
      > div
        background-image url('./img/4-专利数据库.png')
    > li:nth-child(5)
      padding-left 16px
      > div
        background-image url('./img/5-智金大讲堂.png')

