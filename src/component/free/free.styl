#free
  .free-list
    display flex
    flex-wrap wrap
    > li
      flex-basis 50%
      width 160px
      height 160px
      padding 8px 8px
      > div
        width 100%
        height 100%
        background-position center
        background-size: cover;
        background-repeat no-repeat
    > li:nth-child(1)
      padding-left 16px
      > div
        background-image url('./img/1-PCT申请支援.png')
    > li:nth-child(2)
      padding-right 16px
      > div
        background-image url('./img/2-马德里商标支援.png')
    > li:nth-child(3)
      padding-left 16px
      > div
        background-image url('./img/3-维权支援.png')
    > li:nth-child(4)
      padding-right 16px
      > div
        background-image url('./img/4-多语支援.png')
    > li:nth-child(5)
      padding-left 16px
      > div
        background-image url('./img/5-优蚁秘书.png')
    > li:nth-child(6)
      padding-right 16px
      > div
        background-image url('./img/6-补贴申报.png')
